<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - ArchitectureTemplate</title>
    <meta name="keywords" content="Architecture, Template" />
    <meta name="description" content="" />
    <meta name="viewport" content="width=device-width, maximum-scale=1, initial-scale=1, user-scalable=0" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />

    <!-- Fonts -->
    <link href="~/wwwroot/css/font-face.css" rel="stylesheet" type="text/css" />

    <!-- css principal -->
    <link href="~/wwwroot/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/wwwroot/css/font-awesome.css" rel="stylesheet" type="text/css" />
    <link href="~/wwwroot/plugins/owl-carousel/owl.carousel.css" rel="stylesheet" type="text/css" />
    <link href="~/wwwroot/plugins/owl-carousel/owl.theme.css" rel="stylesheet" type="text/css" />
    <link href="~/wwwroot/plugins/owl-carousel/owl.transitions.css" rel="stylesheet" type="text/css" />
    @*<link href="~/wwwroot/plugins/magnific-popup/magnific-popup.css" rel="stylesheet" type="text/css" />*@
    <link href="~/wwwroot/css/animate.css" rel="stylesheet" type="text/css" />
    <link href="~/wwwroot/css/superslides.css" rel="stylesheet" type="text/css" />
    <link href="~/wwwroot/css/site.css" rel="stylesheet" type="text/css" />

    <!-- REVOLUTION SLIDER -->
    <link href="~/wwwroot/plugins/revolution-slider/css/settings.css" rel="stylesheet" type="text/css" />

    <!-- css -->
    <link href="~/wwwroot/css/essentials.css" rel="stylesheet" type="text/css" />
    <link href="~/wwwroot/css/layout.css" rel="stylesheet" type="text/css" />
    <link href="~/wwwroot/css/layout-responsive.css" rel="stylesheet" type="text/css" />

    <!-- Morenizr -->
    <script type="text/javascript" src="~/wwwroot/plugins/modernizr.min.js"></script>
</head>
<body>
    <header id="topHead" class="gs">
        <div class="container">
            <span class="quick-contact pull-left">
                <i class="fa fa-phone"></i> (11) 2816-7249 &bull;
                <a class="hidden-xs" href="mailto:contato@aderbalfarias.com.br">contato@aderbalfarias.com.br</a>
            </span>
            <!-- Login -->
            @Html.Partial(Url.Content("~/Views/Login/_Logar.cshtml"))
        </div>
    </header>

    <header id="sectionAlert" class="container section-alert">
        @Html.Partial("_Message") @*Header de mensagens*@
    </header>

    <header id="topNav" class="topHead">
        <div class="container">

            <!-- Mobile Menu Button -->
            <button class="btn btn-mobile" data-toggle="collapse" data-target=".nav-main-collapse">
                <i class="fa fa-bars"></i>
            </button>

            <!-- Logo text or image -->
            <a class="logo pull-left" href="@Url.Action("Index", "Login")">
                <img src="~/wwwroot/images/logo.png" alt="ArchitectureTemplate" />
            </a>

            <div class="navbar-collapse nav-main-collapse collapse pull-left">
                <nav class="nav-main mega-menu display-none">
                    <ul class="nav nav-pills nav-main scroll-menu" id="topMain">
                        <li>
                            <a href="@Url.Action("About", "Info")">Sobre o ArchitectureTemplate</a>
                        </li>
                    </ul>
                </nav>
            </div>
        </div>
    </header>

    <span id="header_shadow"></span>

    <div id="showSwitcher" class="styleSecondColor">
        <i class="fa fa-angle-double-right"></i>
    </div>

    <!-- Conteúdo -->
    @RenderBody()
    <!-- /Conteúdo -->

    <footer>
        <div class="footer-bar">
            <div class="container">
                <span class="copyright">Copyright © Farias Solutions. Todos os direitos reservados.</span>
                <a class="toTop" href="#topNav">VOLTAR AO TOPO <i class="fa fa-arrow-circle-up"></i></a>
            </div>
        </div>

        <div class="footer-content">
            <div class="container">
                <div class="row">
                    <div class="column col-md-4">
                        <h3>CONTATO</h3>
                        <p class="contact-desc">
                            <a href="http://www.fariassolution.com.br/" target="_blank">
                                my website
                            </a>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </footer>

    <!-- js -->
    <script type="text/javascript" src="~/wwwroot/plugins/jquery-2.1.4.min.js"></script>
    <script type="text/javascript" src="~/wwwroot/plugins/jquery.easing.1.3.js"></script>
    <script type="text/javascript" src="~/wwwroot/plugins/jquery.cookie.js"></script>
    <script type="text/javascript" src="~/wwwroot/plugins/jquery.appear.js"></script>
    <script type="text/javascript" src="~/wwwroot/plugins/jquery.isotope.js"></script>
    <script type="text/javascript" src="~/wwwroot/plugins/masonry.js"></script>
    <script type="text/javascript" src="~/wwwroot/js/layout.js"></script>

    <script type="text/javascript" src="~/wwwroot/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/wwwroot/js/scripts.js@("?v201708141110?")"></script>
    @RenderSection("scripts", required: false)
    @Html.Partial("_ShowMessage")
</body>
</html>